{
  "gpt-context-generator.detectedFileExtensions": [
    "js",
    "jsx",
    "ts",
    "tsx",
    "mdx",
    "json",
    "swift",
    "plist",
    "pbxproj",
    "entitlements"
  ],
  "markdownlint.config": {
    "default": true,
    "MD013": false,
    "MD033": false,
    "MD034": false,
    "MD041": false
  },
  "cSpell.words": ["Blankie"],
  "editor.codeActionsOnSave": {
    "source.fixAll": "always"
  }
}
